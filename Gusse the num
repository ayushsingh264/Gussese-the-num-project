import random
n = random.randint(1,100)
# 
a = 0
guesses = 1

while(a != n):
    a = int(input("Guess the num: "))
    if(a>n):
        print("Lower num pls..")
        guesses += 1
    elif(a<n):
        print("Greater num pls..")
        guesses +=1
print(f"You have guessed the num {n} correctly in {guesses} attempts")
print("Thanku")
